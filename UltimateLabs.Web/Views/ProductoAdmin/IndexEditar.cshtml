@model UltimateLabs.Web.Models.ProductoAdminViewModel
@{
    /**/

    ViewBag.Title = "IndexEditar";
    Layout = "~/Views/Shared/_LayoutTry.cshtml";
}

<style>
    .pagination > li > a, .pagination > li > span {
        color: white !Important;
        background-color: #02779d !important;
    }

    .boton {
        background-color: white;
    }

    .fontbody {
        font-family: Seagoe UI, sans-serif;
    }
</style>
<body class="fontbody">

    <section style="background-color:#">
        <h4 style="color:#02779d;margin-left:15%">Productos > Crear</h4>
        <h3 style="color:#02779d;margin-left:15%">Crear</h3>
        <div class="row">
            <div style="margin-top:-1%;margin-left:14% !important">
                <div class="col-lg-12 col-md-12 col-xs-12">
                    @{ Html.RenderAction("EditarProducto", new { id = Model.CodigoMaestro });}
                </div>


            </div>
        </div>

    </section>
</body>

